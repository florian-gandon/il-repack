<Window x:Class="WPFSampleApplicationCore.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPFSampleApplicationCore" 
        xmlns:classlibrarycore="clr-namespace:ClassLibraryCore;assembly=ClassLibraryCore"
        mc:Ignorable="d"
        Title="MainWindow" Height="268" Width="800"
        Loaded="MainWindow_Loaded">
    <StackPanel Background="AliceBlue">
        <Button Content="Button" HorizontalAlignment="Left" VerticalAlignment="Top"/>
        <TextBox HorizontalAlignment="Left" Text="TextBox" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <classlibrarycore:DummyUserControl />

    </StackPanel>
</Window>
